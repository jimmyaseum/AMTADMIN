<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_30sdp"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginRight="@dimen/_15sdp">

            <TextView
                android:id="@+id/txtTourBookingNo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_semibold"
                android:text=""
                android:textColor="@color/colorAccent"
                android:textSize="@dimen/_17sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"/>

            <!--First Name-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="First Name"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>
                <EditText
                    android:id="@+id/edtFirstName"
                    style="@style/TextViewStyleRegular"
                    android:hint=""
                    android:imeOptions="actionNext"
                    android:background="@drawable/aaa"
                    android:inputType="textCapCharacters" />
            </LinearLayout>

            <!--Last Name-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="Last Name"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>
                <EditText
                    android:id="@+id/edtLastName"
                    style="@style/TextViewStyleRegular"
                    android:hint=""
                    android:imeOptions="actionNext"
                    android:background="@drawable/aaa"
                    android:inputType="textCapCharacters" />
            </LinearLayout>

            <!--Email-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="Email"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>
                <EditText
                    android:id="@+id/edtEmail"
                    style="@style/TextViewStyleRegular"
                    android:hint=""
                    android:background="@drawable/aaa"
                    android:inputType="textPersonName|textCapSentences" />
            </LinearLayout>

            <!--Mobile Number-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="Mobile Number"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>
                <EditText
                    android:id="@+id/edtMobileNo"
                    style="@style/TextViewStyleRegular"
                    android:clickable="false"
                    android:enabled="false"
                    android:editable="false"
                    android:focusable="false"
                    android:background="@drawable/aaa"
                    android:inputType="number"
                    android:maxLength="15"/>
            </LinearLayout>

            <!--Address-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="Address"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>
                <EditText
                    android:id="@+id/edtAddress"
                    style="@style/TextViewStyleRegular"
                    android:hint=""
                    android:background="@drawable/aaa"
                    android:inputType="textCapCharacters"/>
            </LinearLayout>

            <!--City-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="City"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>
                <EditText
                    android:id="@+id/edtCity"
                    style="@style/TextViewStyleRegular"
                    android:hint=""
                    android:editable="false"
                    android:enabled="true"
                    android:focusable="false"
                    android:clickable="true"
                    android:background="@drawable/aaa"
                    android:drawableRight="@drawable/ic_arrow_drop_down"
                    android:drawableTint="@color/gray8" />
            </LinearLayout>

            <!--State-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="State"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>
                <EditText
                    android:id="@+id/edtState"
                    style="@style/TextViewStyleRegular"
                    android:hint=""
                    android:editable="false"
                    android:enabled="false"
                    android:focusable="false"
                    android:clickable="false"
                    android:background="@drawable/aaa"/>
            </LinearLayout>

            <!--Country-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="Country"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>
                <EditText
                    android:id="@+id/edtCountry"
                    style="@style/TextViewStyleRegular"
                    android:hint=""
                    android:editable="false"
                    android:enabled="false"
                    android:focusable="false"
                    android:clickable="false"
                    android:background="@drawable/aaa" />

            </LinearLayout>

            <!--Travelling MobileNo-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="Travelling MobileNo"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>
                <EditText
                    android:id="@+id/edtTravellingMobileNo"
                    style="@style/TextViewStyleRegular"
                    android:hint=""
                    android:maxLength="15"
                    android:background="@drawable/aaa"
                    android:imeOptions="actionNext"
                    android:inputType="number" />
            </LinearLayout>

            <!--ResidentPhoneNo-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="Resident PhoneNo"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>
                <EditText
                    android:id="@+id/edtResidentPhoneNo"
                    style="@style/TextViewStyleRegular"
                    android:hint=""
                    android:background="@drawable/aaa"
                    android:maxLength="15"
                    android:imeOptions="actionNext"
                    android:inputType="number" />

            </LinearLayout>

            <!--EmergencyNo-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="Emergency No"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>
                <EditText
                    android:id="@+id/edtEmergencyNo"
                    style="@style/TextViewStyleRegular"
                    android:hint=""
                    android:maxLength="15"
                    android:background="@drawable/aaa"
                    android:imeOptions="actionNext"
                    android:inputType="number" />
            </LinearLayout>

            <!--PanNo-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="Pan No"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>
                <EditText
                    android:id="@+id/edtPanNo"
                    style="@style/TextViewStyleRegular"
                    android:hint=""
                    android:background="@drawable/aaa"
                    android:imeOptions="actionNext"
                    android:inputType="textCapCharacters" />
            </LinearLayout>

            <!--PassPortNo-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="PassPort No"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>
                <EditText
                    android:id="@+id/edtPassPortNo"
                    style="@style/TextViewStyleRegular"
                    android:hint=""
                    android:background="@drawable/aaa"
                    android:imeOptions="actionNext"
                    android:inputType="textCapCharacters" />
            </LinearLayout>

            <!--AadharNo-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="Aadhar No"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>
                <EditText
                    android:id="@+id/edtAadharNo"
                    style="@style/TextViewStyleRegular"
                    android:hint=""
                    android:background="@drawable/aaa"
                    android:imeOptions="actionNext"
                    android:inputType="textCapCharacters" />
            </LinearLayout>

            <!--IsGST-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/poppins_semibold"
                    android:text="Is Company GST?"
                    android:textColor="@color/gray8"
                    android:textSize="@dimen/_13sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_marginLeft="@dimen/_10sdp"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/_5sdp"
                    android:layout_marginLeft="@dimen/_5sdp">

                    <CheckBox
                        android:id="@+id/cbYes"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:text="Yes"
                        android:fontFamily="@font/poppins_medium"
                        android:textSize="@dimen/_13sdp"
                        android:buttonTint="@color/colorAccent"
                        android:textColor="@color/gray8"/>
                    <CheckBox
                        android:id="@+id/cbNo"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:text="No"
                        android:fontFamily="@font/poppins_medium"
                        android:textSize="@dimen/_13sdp"
                        android:buttonTint="@color/colorAccent"
                        android:textColor="@color/gray8" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/LL_Company"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <!--Company Name-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_semibold"
                        android:text="Company Name"
                        android:textColor="@color/gray8"
                        android:textSize="@dimen/_13sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_10sdp"/>
                    <EditText
                        android:id="@+id/edtCompanyName"
                        style="@style/TextViewStyleRegular"
                        android:hint=""
                        android:imeOptions="actionNext"
                        android:background="@drawable/aaa"
                        android:inputType="textCapCharacters" />
                </LinearLayout>

                <!--Company Address-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_semibold"
                        android:text="Company Address"
                        android:textColor="@color/gray8"
                        android:textSize="@dimen/_13sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_10sdp"/>
                    <EditText
                        android:id="@+id/edtCompanyAddress"
                        style="@style/TextViewStyleRegular"
                        android:hint=""
                        android:background="@drawable/aaa"
                        android:inputType="textCapCharacters"/>
                </LinearLayout>

                <!--GST No-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_semibold"
                        android:text="Company GST No"
                        android:textColor="@color/gray8"
                        android:textSize="@dimen/_13sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_10sdp"/>
                    <EditText
                        android:id="@+id/edtCompanyGSTNo"
                        style="@style/TextViewStyleRegular"
                        android:hint=""
                        android:textColor="@color/colorBlack"
                        android:background="@drawable/aaa"
                        android:inputType="textCapCharacters"/>
                </LinearLayout>

                <!--PAN No-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_semibold"
                        android:text="Company Pan No"
                        android:textColor="@color/gray8"
                        android:textSize="@dimen/_13sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_10sdp"/>
                    <EditText
                        android:id="@+id/edtCompanyPANNo"
                        style="@style/TextViewStyleRegular"
                        android:hint=""
                        android:inputType="textCapCharacters"
                        android:textColor="@color/colorBlack"
                        android:background="@drawable/aaa"/>
                </LinearLayout>

                <!--City-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_semibold"
                        android:text="Company City"
                        android:textColor="@color/gray8"
                        android:textSize="@dimen/_13sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_10sdp"/>
                    <EditText
                        android:id="@+id/edtCompanyCity"
                        style="@style/TextViewStyleRegular"
                        android:hint=""
                        android:editable="false"
                        android:enabled="true"
                        android:focusable="false"
                        android:clickable="true"
                        android:background="@drawable/aaa"
                        android:drawableRight="@drawable/ic_arrow_drop_down"
                        android:drawableTint="@color/gray8"/>
                </LinearLayout>

                <!--State-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_semibold"
                        android:text="Company State"
                        android:textColor="@color/gray8"
                        android:textSize="@dimen/_13sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_10sdp"/>
                    <EditText
                        android:id="@+id/edtCompanyState"
                        style="@style/TextViewStyleRegular"
                        android:hint=""
                        android:editable="false"
                        android:enabled="false"
                        android:focusable="false"
                        android:clickable="false"
                        android:background="@drawable/aaa" />
                </LinearLayout>

                <!--Country-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/poppins_semibold"
                        android:text="Company Country"
                        android:textColor="@color/gray8"
                        android:textSize="@dimen/_13sdp"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginLeft="@dimen/_10sdp"/>
                    <EditText
                        android:id="@+id/edtCompanyCountry"
                        style="@style/TextViewStyleRegular"
                        android:hint=""
                        android:editable="false"
                        android:enabled="false"
                        android:focusable="false"
                        android:clickable="false"
                        android:background="@drawable/aaa"/>

                </LinearLayout>

            </LinearLayout>

            <CheckBox
                android:id="@+id/cbIsCustomerUpdate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Update data in customer Details"
                android:fontFamily="@font/poppins_medium"
                android:textSize="@dimen/_13sdp"
                android:buttonTint="@color/colorAccent"
                android:textColor="@color/gray8"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_8sdp"
                android:visibility="gone"/>

            <androidx.cardview.widget.CardView
                android:id="@+id/LLcardButtonNext"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardBackgroundColor="@color/colorAccent"
                app:cardCornerRadius="@dimen/_5sdp"
                android:elevation="@dimen/_10sdp"
                android:layout_margin="@dimen/_10sdp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_gravity="center">

                    <TextView
                        style="@style/buttonText"
                        android:text="Save" />
                </LinearLayout>

            </androidx.cardview.widget.CardView>

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</LinearLayout>

